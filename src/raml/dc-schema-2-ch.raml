 - addresses: |
    {
      "type": "object",
      "$schema": "http://json-schema.org/draft-04/schema",
      "id": "http://doccirrus.com/schema/addresses",
      "required": true,
      "properties": {
                          "street": {
                    "type": "string",
                    "validate": [
                              {
                                        "msg": "Das Feld \"{PATH}\" muss 1-46 Zeichen lang sein."
                              }
                    ]
          },
          "houseno": {
                    "type": "string",
                    "validate": [
                              {
                                        "msg": "Das Feld darf nicht leer bleiben."
                              },
                              {
                                        "msg": "Das Feld \"{PATH}\" muss 0-9 Zeichen lang sein."
                              }
                    ],
                    "description": "House No."
          },
          "zip": {
                    "type": "string",
                    "validate": [
                              {
                                        "msg": "Der angegebene Wert ist keine gültige Postleitzahl. Verwenden Sie nur gängige Schreibweisen wie, z.B. 1210."
                              }
                    ]
          },
          "city": {
                    "type": "string",
                    "required": true,
                    "validate": [
                              {
                                        "msg": "Das Feld \"{PATH}\" muss 0-40 Zeichen lang sein."
                              }
                    ]
          },
          "postbox": {
                    "type": "string",
                    "validate": [
                              {
                                        "msg": "Das Feld \"{PATH}\" darf nicht leer bleiben."
                              }
                    ]
          },
          "kind": {
                    "enum": [
                              "OFFICIAL",
                              "POSTBOX",
                              "POSTAL",
                              "BILLING",
                              "VISIT",
                              "BRANCH",
                              "EMPLOYER"
                    ],
                    "required": true
          },
          "country": {
                    "type": "string"
          },
          "countryCode": {
                    "type": "string",
                    "description": "Country code"
          },
          "receiver": {
                    "type": "string",
                    "description": "Addressee"
          },
          "payerType": {
                    "type": "string"
          },
          "addon": {
                    "type": "string",
                    "description": "Address addition"
          },
          "talk": {
                    "enum": [
                              "MR",
                              "MS",
                              "NONE"
                    ]
          },
          "title": {
                    "type": "string"
          },
          "firstname": {
                    "type": "string",
                    "description": "First Name"
          },
          "nameaffix": {
                    "type": "string",
                    "description": "Name Affix"
          },
          "middlename": {
                    "type": "string",
                    "description": "Middle Name"
          },
          "lastname": {
                    "type": "string",
