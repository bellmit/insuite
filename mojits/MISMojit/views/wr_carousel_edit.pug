.modal.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true")#myModal
  .modal-dialog
    .modal-content
      form#pForm(action="/waitingroom?action=upload_carousel", method="post", enctype="multipart/form-data")
      input(type="hidden", name="setId", value="").setId
      .modal-header
        button.btn(type="button", class="close", data-dismiss="modal", aria-hidden="true")#btnModalClose Ã—
        h3#myModalLabel.modal-title(data-bind="text: titleEditImagesI18n")
      .modal-body
        .row
          .col-xs-11.col-xs-offset-1
            p(data-bind="text: lblUploadInstructionsI18n")
        for item in [0, 1, 2, 3, 4]
          .row
            .col-xs-1.col-xs-offset-2
              .showbox
                img(src="", style="max-width: 100%")#thumb
            .col-xs-9
              input(type='file', accept='image/*', capture='camera', name="photo")#upload
              input(type="hidden", value="")#filename
          .row
            .col-xs-1.col-xs-offset-2
            .col-xs-9
              button.btn.btn-default.btn-sm( data-bind="text: btnResetI18n")#resetbtn

      .modal-footer
        button.btn.btn-default(data-dismiss="modal", aria-hidden="true", data-bind="text: btnSaveI18n")#btnReload
        button.btn.btn-primary(disabled="true", style="display:none;", data-bind="text: btnSaveI18n")#savebtn
