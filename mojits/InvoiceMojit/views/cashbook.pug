div#cashBook.row(data-bind="visible: isVisible", style={display: 'none'})
  div.dc-action-buttons.dc-action-buttons-top.dc-action-buttons-left.clearfix
    div.col-xs-12
      div.col-lg-8.col-md-9.col-xs-12(data-bind="template: dateSelector.template")
      div.col-lg-4.col-md-3(style="margin-top: 20px; display: inline-block;")
        div.checkbox(style="margin-bottom: 0;")
          label
            span(data-bind="text: showAutoGenInvoicesI18n")
            input(type="checkbox", data-bind="checked: showAutoGeneratedOnly")
        div.checkbox(style="margin-bottom: 0;")
          label
          span(data-bind="text: notShowCanceledI18n")
          input(type="checkbox", data-bind="checked: notShowCanceled")
  div.row
    div.col-xs-12
      div#progressbarProcess(data-bind="visible: inProcess")
        div.progress(style="margin-top: 8px;")
          div.progress-bar(style="padding-left: 8px; text-align: left;", role="progressbar", aria-valuenow="0", aria-valuemin="0", aria-valuemax="100", data-bind="text: progressBarProcess.text,style: {width: progressBarProcess.progress() + '%'}")
    div.col-xs-12
      div#progressbarDocument(data-bind="visible: showPdfBar")
        div.progress(style="margin-top: 8px;")
          div.progress-bar(style="padding-left: 8px; text-align: left;", role="progressbar", aria-valuenow="0", aria-valuemin="0", aria-valuemax="100", data-bind="text: progressBarDocument.text,style: {width: progressBarDocument.progress() + '%'}")


  div(data-bind="template: invoiceKoTable.template")
  div.dc-action-buttons.dc-action-buttons-left(data-bind="foreach: transitionButtons")
    span(data-bind="template: $data.template", style="margin-bottom:10px; display:inline-block;")

  div(style="display: none;")#divIfDownload