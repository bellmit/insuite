  .form-group
    div.checkbox
      label.control-label
        input(name="statusFilter", type="checkbox", data-bind="checked: statusFilter")
        span(data-bind="text: statusI18n")
  .form-group
    div.checkbox
      label.control-label
        input(name="patientNoFilter", type="checkbox", data-bind="checked: patientNoFilter")
        span(data-bind="text: patientNoI18n")
  .form-group
    div.checkbox
      label.control-label
        input(name="patientDobFilter", type="checkbox", data-bind="checked: patientDobFilter")
        span(data-bind="text: patientDobI18n")
  h4(data-bind="text: roomI18n")
  .form-group
    label.radio-inline
      input(type="radio", name="columns", value= "2", data-bind="checked: columnsFilter")
      span(data-bind="text: columns2I18n")
  .form-group
    label.radio-inline
      input(type="radio", name="columns", value= "3", data-bind="checked: columnsFilter")
      span(data-bind="text: columns3I18n")
  // /ko
