div(style="border: 1px; border-color: black;")#divFormsComposeFrame
            div(style='background-color:#555555;padding:10px;')#divFormButtons
                span#spanFillButtons

                    button.btn(type="button")#btnPrev
                        i.fa.fa-arrow-left
                        | &nbsp;
                        span(data-bind="text: btnPreviousI18n")
                    span &nbsp;

                    button.btn(type="button")#btnNext
                        i.fa.fa-arrow-right
                        | &nbsp;
                        span(data-bind="text: btnNextI18n")
                    span &nbsp;

                    button.btn(type="button")#btnSubmit
                        i.fa.fa-save
                        | &nbsp;
                        span(data-bind="text: btnSubmitI18n")
                    span &nbsp;

                    button.btn(type="button",style="display: none;")#btnPDF
                        i.fa.fa-print
                        | &nbsp;
                        span(data-bind="text: btnPdfI18n")
                    span &nbsp;

                span(style="float:right;color:#aaaaaa;")
                    span(style="position: relative; top: 5px;")#spanStatus
                    img(src="/static/FormEditorMojit/assets/images/throbber.gif",style="float:right;position:relative;top:5px;")#imgThrobber

            div#divFormsCompose

div#divFormsRender