script(type="text/html", id="PatientDiffModel")
  div.table-responsive
    table.table.DCWindow-DiffPatient
      colgroup
        col(style="width:30%")
        col(style="width:35%; border-left: 1px solid #ddd;")
        col(style="width:35%; border-left: 1px solid #ddd;")
      tbody(data-bind="foreach: categories")
        tr
          th(colspan="3")
            h4(data-bind="text: title")
        tr
          th
          th(data-bind="text: $parent.selPatientI18n")
          th
            span(data-bind="text: $parent.newPatientI18n")
            span(data-bind="if: 'insuranceStatus' === pathPart && $parent.showCardInsuranceIgnoredHint")
              i.dc-info-icon(data-bind="tooltip: {title: $parents[1].i18n.CARD_INSURANCE_DELETED_INFO, placement: 'top', trigger:'click'}")
            span(data-bind="if: 'insuranceStatus' === pathPart && $parent.showCardInsuranceCardSwipeIgnoredHint")
              i.dc-info-icon(data-bind="tooltip: {title: $parents[1].i18n.CARDSWIPE_DELETED_INFO, placement: 'top', trigger:'click'}")
        // ko foreach: data
        // ko if: separator
        tr
          th
          th &nbsp;
          th &nbsp;
        // /ko
        // ko ifnot: separator
        tr(data-bind="css: (changed?'row-conflict':'row-ok')")
          th(data-bind="text: label")
          td.bg-conflict(data-bind="text: bVal")
          td.bg-noconflict(data-bind="text: aVal")
        // /ko
        // /ko
