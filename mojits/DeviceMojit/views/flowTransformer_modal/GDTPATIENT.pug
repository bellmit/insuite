div#transformerModel
  div.form-group
    h4(data-bind="text: patientNoI18n + ':'")
    span.checkbox
      label.control-label
        strong(data-bind="text: showOriginalIdI18n")
        input(type="checkbox", name="transformerShowOriginalId", data-bind="checked: showOriginalId")
    label(for="transformerSelect2GdtVersion", data-bind="text: gdtVersionI18n + ':'")
    input.form-control.input-sm#transformerSelect2GdtVersion(type="hidden", data-bind="select2: select2GdtExportVersion, attr: { gdtVersion }")
    label(for="transformerSelect2GdtEncoding", data-bind="text: gdtEncodingI18n + ':'")
    input.form-control.input-sm#transformerSelect2GdtEncoding(type="hidden", data-bind="select2: select2GdtExportEncoding")
    label(for="transformerGdtSender", data-bind="text: gdtSenderI18n + ':'")
    input.form-control.input-md#transformerGdtSender(data-bind="value: gdtSender, valueUpdate:'afterkeydown'")
    label(for="transformerGdtReceiver", data-bind="text: gdtReceiverI18n + ':'")
    input.form-control.input-md#transformerGdtReceiver(data-bind="value: gdtReceiver, valueUpdate:'afterkeydown'")
    br
    h4(data-bind="text: medicineDataI18n + ':'")
    span(data-bind="text: medicineDataSubText")
    span.checkbox
      label.control-label
        strong(data-bind="text: exportMedDataI18n")
        input(type="checkbox", name="exportMedDataBox", data-bind="checked: exportMedData")
      i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': exportMedDataPopoverTitle, 'data-content': exportMedDataPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
    br
    h4(data-bind="text: gdtPatientExportSpecialMatchI18n + ':'")
      h5
        i.fa.fa-exclamation-triangle(aria-hidden="true")
        span(data-bind="text: gdtPatientExportSpecialMatchNoticeI18n + ' ' + gdtPatientExportSpecialMatchAdditionalTextI18n")
    //ko if: gdtVersion() === 'gdt30'
    span.checkbox
      div.col-md-8(style="padding-left: 0px;")
        label.control-label
          input(type="checkbox", name="mapPatientLocationAddonBox", data-bind="checked: mapPatientLocationAddon")
          strong(data-bind="text: mapPatientLocationAddonI18n")
        i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': mapPatientLocationAddonPopoverTitle, 'data-content': mapPatientLocationAddonPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
        strong.control-label(style="margin-left: 5px;", data-bind="text: mappedToI18n")
      div.col-md-4
        input.form-control.input-sm(style="width:70px; display: inline-block; margin-left:10px;", name="mapPatientLocationAddonToBox", data-bind="value: mapPatientLocationAddonTo")
    // /ko
    //ko if: gdtVersion() !== 'gdt30'
    span.checkbox
      div.col-md-9(style="padding-left: 0px;")
        label.control-label
          input(type="checkbox", name="mapBSNRBox", data-bind="checked: mapBSNR")
          strong(data-bind="text: mapBSNRI18n")
        i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': mapBSNRPopoverTitle, 'data-content': mapBSNRPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
        strong.control-label(style="margin-left: 5px;", data-bind="text: mappedToI18n")
      div.col-md-3
        input.form-control.input-sm(style="width:70px; display: inline-block; margin-left:10px;", name="mapBSNRToBox", data-bind="value: mapBSNRTo" disabled)
    span.checkbox
      div.col-md-9(style="padding-left: 0px;")
        label.control-label
          input(type="checkbox", name="mapCaseFolderIdBox", data-bind="checked: mapCaseFolderId")
          strong(data-bind="text: mapCaseFolderIdI18n")
        i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': mapCaseFolderIdPopoverTitle, 'data-content': mapCaseFolderIdPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
        strong.control-label(style="margin-left: 5px;", data-bind="text: mappedToI18n")
      div.col-md-3
        input.form-control.input-sm(style="width:70px; display: inline-block; margin-left:10px;", name="mapCaseFolderIdToBox", data-bind="value: mapCaseFolderIdTo" disabled)
    span.checkbox
      div.col-md-9(style="padding-left: 0px;")
        label.control-label
          input(type="checkbox", name="mapResponsibleDoctorBox", data-bind="checked: mapResponsibleDoctor")
          strong(data-bind="text: mapResponsibleDoctorI18n")
        i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': mapResponsibleDoctorPopoverTitle, 'data-content': mapResponsibleDoctorPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
        strong.control-label(style="margin-left: 5px;", data-bind="text: mappedToI18n")
      div.col-md-3
        input.form-control.input-sm(style="width:70px; display: inline-block; margin-left:10px;", name="mapResponsibleDoctorToBox", data-bind="value: mapResponsibleDoctorTo" disabled)
    span.checkbox
      div.col-md-9(style="padding-left: 0px;")
        label.control-label
          input(type="checkbox", name="mapEmployeeIdBox", data-bind="checked: mapEmployeeId")
          strong(data-bind="text: mapEmployeeIdI18n")
        i.dc-info-icon(data-toggle="popover", data-bind="attr: {'data-original-title': mapEmployeeIdPopoverTitle, 'data-content': mapEmployeeIdPopover }", data-html="true", rel="popover", data-placement="auto left", data-trigger="click")
        strong.control-label(style="margin-left: 5px;", data-bind="text: mappedToI18n")
      div.col-md-3
        input.form-control.input-sm(style="width:70px; display: inline-block; margin-left:10px;", name="mapEmployeeIdToBox", data-bind="value: mapEmployeeIdTo" disabled)
    // /ko