div#tab_inpacslog_import
    h3(data-bind="text: titleI18n")
    #dicomFineUploader.filesDropContainer
    span &nbsp;
    #dicomUploadStatus
    #dicomUploadResult(style="white-space: pre;")

    script(type="text/template")#qq-template-dicomupload-status
        .qq-uploader-selector.qq-uploader.qq-gallery( style="height: 100%;")
            .qq-upload-drop-area-selector.qq-upload-drop-area(qq-hide-dropzone, style="background-color: rgba(100, 255, 100, 0.3);")
                span.qq-upload-drop-area-text-selector
            div.btn.btn-default.qq-upload-button-selector.qq-upload-button.uploadDicomButton#btnUploadFine( data-bind="enable: uploadEnabled" )
                i.fa.fa-cloud-upload
                | &nbsp;
                span(data-bind="text: buttonUploadI18n")
            br
            span.dropDicomFilesInfo( data-bind="attr: { style: showDropFilesText}, text: dropFilesHereTextI18n" )
            .qq-total-progress-bar-container-selector.qq-total-progress-bar-container.progressBar
                span(data-bind="text: uploadingTextI18n")
                .qq-total-progress-bar-selector.qq-progress-bar.qq-total-progress-bar(role='progressbar', aria-valuenow='0', aria-valuemin='0', aria-valuemax='100')
            span.qq-drop-processing-selector.qq-drop-processing
                span Processing dropped files...
                span.qq-drop-processing-spinner-selector.qq-drop-processing-spinner
            ul.qq-upload-list-selector.qq-upload-list.qq-hide(role='region', aria-live='polite', aria-relevant='additions removals')
                li
                    span.qq-upload-status-text-selector.qq-upload-status-text(role='status')
                    .qq-progress-bar-container-selector.qq-progress-bar-container
                        .qq-progress-bar-selector.qq-progress-bar(role='progressbar', aria-valuenow='0', aria-valuemin='0', aria-valuemax='100')
                    span.qq-upload-spinner-selector.qq-upload-spinner
                    .qq-thumbnail-wrapper
                        img.qq-thumbnail-selector(qq-max-size='120', qq-server-scale='')
                    button.qq-upload-cancel-selector.qq-upload-cancel(type='button') X
                    button.qq-upload-retry-selector.qq-upload-retry(type='button')
                        span.qq-btn.qq-retry-icon(aria-label='Retry')
                        |                         Retry
                    .qq-file-info
                        .qq-file-name
                            span.qq-upload-file-selector.qq-upload-file
                            span.qq-edit-filename-icon-selector.qq-btn.qq-edit-filename-icon(aria-label='Edit filename')
                        input.qq-edit-filename-selector.qq-edit-filename(tabindex='0', type='text')
                        span.qq-upload-size-selector.qq-upload-size
                        button.qq-btn.qq-upload-delete-selector.qq-upload-delete(type='button')
                            span.qq-btn.qq-delete-icon(aria-label='Delete')
                        button.qq-btn.qq-upload-pause-selector.qq-upload-pause(type='button')
                            span.qq-btn.qq-pause-icon(aria-label='Pause')
                        button.qq-btn.qq-upload-continue-selector.qq-upload-continue(type='button')
                            span.qq-btn.qq-continue-icon(aria-label='Continue')
            dialog.qq-alert-dialog-selector
                .qq-dialog-message-selector
                .qq-dialog-buttons
                    button.qq-cancel-button-selector(type='button') Close
            dialog.qq-confirm-dialog-selector
                .qq-dialog-message-selector
                .qq-dialog-buttons
                    button.qq-cancel-button-selector(type='button') No
                    button.qq-ok-button-selector(type='button') Yes
            dialog.qq-prompt-dialog-selector
                .qq-dialog-message-selector
                input(type='text')
                .qq-dialog-buttons
                    button.qq-cancel-button-selector(type='button') Cancel
                    button.qq-ok-button-selector(type='button') Ok